{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "KAIGANGAMESWEBSITE_A15_SASS": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/kaigangameswebsite-a15-sass",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "sass",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.sass",
              "src/assets/backend/sass/app.scss",
              "./node_modules/sweetalert2/src/sweetalert2.scss",
              "./node_modules/bootstrap/dist/css/bootstrap.css",
              "./node_modules/ngx-toastr/toastr.css",
              "node_modules/aos/dist/aos.css",
              "node_modules/datatables.net-bs5/css/dataTables.bootstrap5.min.css",
              "src/assets/backend/plugins/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/primeng/resources/themes/lara-light-indigo/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@danielmoncada/angular-datetime-picker/assets/style/picker.min.css",
              "./node_modules/select2/dist/css/select2.min.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/backend/plugins/simplebar/js/simplebar.min.js",
              "src/assets/backend/js/app.js",
              "src/assets/backend/plugins/metismenu/js/metisMenu.min.js",
              "src/assets/backend/plugins/perfect-scrollbar/js/perfect-scrollbar.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js",
              "node_modules/datatables.net-bs5/js/dataTables.bootstrap5.min.js",
              "src/assets/backend/plugins/perfect-scrollbar/js/perfect-scrollbar.js",
              "node_modules/slick-carousel/slick/slick.min.js"
            ],
            "allowedCommonJsDependencies": [
              "sweetalert2",
              "jquery",
              "aos",
              "photoswipe",
              "file-saver"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "1750kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "30kb",
                  "maximumError": "50kb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "optimization": {
                "scripts": {
                  "minify": true,
                  "inlineCritical": false
                },
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "aot": true,
              "buildOptimizer": true,
              "vendorChunk": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "KAIGANGAMESWEBSITE_A15_SASS:build:production"
            },
            "development": {
              "browserTarget": "KAIGANGAMESWEBSITE_A15_SASS:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "KAIGANGAMESWEBSITE_A15_SASS:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "sass",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.sass"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
